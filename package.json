{
  "name": "backend-57block",
  "version": "1.0.0",
  "description": "Prueba tecnica backend 57block",
  "main": "build/index.js",
  "scripts": {
    "prepare": "husky install",
    "build": "tsc",
    "dev": "DEBUG=Server:* NODE_ENV=dev nodemon",
    "debug": "DEBUG=Server:* NODE_ENV=dev node --inspect-brk node_modules/.bin/ts-node ./index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier:format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "test": "NODE_ENV=test jest",
    "test:debug": "ENV_NODE=test && node --inspect-brk node_modules/.bin/jest",
    "coverage": "jest --coverage",
    "migration:up": "knex --knexfile=./src/infrastructure/orm/knexfile.ts migrate:latest",
    "migration:create": "knex --knexfile=./src/infrastructure/orm/knexfile.ts migrate:make",
    "migration:rollback": "knex --knexfile=./src/infrastructure/orm/knexfile.ts migrate:rollback",
    "migration:rollback:all": "knex --knexfile=./src/infrastructure/orm/knexfile.ts migrate:rollback --all",
    "migrate:list": "knex --knexfile=./src/infrastructure/orm/knexfile.ts migrate:list",
    "seed:create": "knex --knexfile=./src/infrastructure/orm/knexfile.ts seed:make",
    "seed:run": "knex --knexfile=./src/infrastructure/orm/knexfile.ts seed:run"
  },
  "author": "Maestroshifu",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.1",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.0",
    "@types/js-yaml": "^4.0.2",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/node": "^16.4.13",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "jest": "27.0.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "supertest": "^6.1.5",
    "ts-jest": "27.0.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-openapi-validate": "^0.6.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.21",
    "objection": "^2.2.15",
    "pg": "^8.7.1",
    "swagger-ui-express": "^4.1.6"
  }
}
